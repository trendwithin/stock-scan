<h1 class="center">Current New Highs/NewLows</h1>
<div class="row">
  <div class="container">
    <table class=" table table-striped table-bordered">
      <tr>
        <th class="center">Date</th>
        <th class="center">One Month High</th>
        <th class="center">One Month Low</th>
        <th class="center">Three Month High</th>
        <th class="center">Three Month Low</th>
        <th class="center">Six Month High</th>
        <th class="center">Six Month Low</th>
        <th class="center">Twelve Month High</th>
        <th class="center">Twelve Month Low</th>
        <th class="center">YTD  High</th>
        <th class="center">YTD Low</th>
      </tr>

      <% @hl.each do |e| %>
      <tr>
        <td><%= e.saved_on %></td>
        <td><%= e.one_month_high %></td>
        <td><%= e.one_month_low %></td>
        <td><%= e.three_month_high %></td>
        <td><%= e.three_month_low %></td>
        <td><%= e.six_month_high %></td>
        <td><%= e.six_month_low %></td>
        <td><%= e.twelve_month_high %></td>
        <td><%= e.twelve_month_low %></td>
        <td><%= e.ytd_high %></td>
        <td><%= e.ytd_low %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
